import java.util.*;
public class FirstCode {
    public static void main (String []args){
        int num;
        int i = 0;

        Scanner sc = new Scanner(System.in);
        System.out.println("Please enter number");
        num = sc.nextInt();

        while (num!= 0){
            num/=10;
            ++i;
        }
        System.out.println("This number has" + " "+ i + " "+ "digits");
    }
}
